{
  "name": "angular2-start",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "postinstall": "typings install",
    "prestart": "npm run build",
    "start": "forever start ./dist/server.js",
    "restart": "forever restart ./dist/server.js",
    "stop": "forever stop ./dist/server.js",
    "watch": "nodemon ./dist/server.js",
    "prebuild": "rimraf ./dist",
    "build": "ng build",
    "postbuild": "tsc -p ./src",
    "lint": "tslint \"src/**/*.ts\"",
    "coverage:coveralls": "coveralls < ./coverage/lcov.info",
    "coverage:remap": "npm run coverage:remap:lcov && npm run coverage:remap:html && npm run coverage:remap:text",
    "coverage:remap:lcov": "remap-istanbul -i ./coverage/coverage-final.json -o ./coverage/lcov.info -t lcovonly",
    "coverage:remap:html": "remap-istanbul -i ./coverage/coverage-final.json -o ./coverage/html -t html",
    "coverage:remap:text": "remap-istanbul -i ./coverage/coverage-final.json -o ./coverage/coverage.txt -t text",
    "coverage:serve": "http-server ./coverage/html -p 3001",
    "coverage:stdout": "cat ./coverage/coverage.txt",
    "pretest": "rimraf ./coverage && npm run build",
    "test": "karma start ./config/karma.conf.js",
    "posttest": "npm run coverage:remap && npm run coverage:stdout",
    "e2e": "bash ./test-e2e.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/service-worker": "0.2.0",
    "angular-cli": "1.0.0-beta.5",
    "angular2-broccoli-prerender": "0.11.0",
    "angular2-universal": "0.102.1",
    "angular2-universal-polyfills": "0.4.1",
    "body-parser": "1.15.1",
    "bootstrap": "3.3.6",
    "es6-promise": "3.2.1",
    "es6-shim": "0.35.0",
    "express": "4.13.4",
    "preboot": "2.1.2",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "codelyzer": "0.0.19",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-spec-reporter": "0.0.26",
    "less": "2.7.1",
    "nodemon": "^1.9.2",
    "protractor": "^3.3.0",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.2",
    "ts-node": "^0.5.5",
    "tslint": "^3.6.0",
    "typescript": "^1.8.10",
    "typings": "^1.1.0"
  }
}
